{
  "name": "eliftech-school---delivery-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cd client && npm install && npm run build",
    "start": "cross-env NODE_ENV=production node ./server/bin/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon ./server/bin/server.js",
    "install-server": "cd server && npm install",
    "start-server": "cd ./server/bin && node server.js",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "start-client": "cd client && npm run start",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start:dev\" \"npm run client\""
  },
